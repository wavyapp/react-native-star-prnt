{
  "name": "@wavyapp/react-native-star-prnt",
  "version": "4.1.4",
  "description": "React Native module for Star Micronics Bluetooth/LAN printers",
  "author": "Sebastien Vray <sebastien.vray@treatwell.com>",
  "license": "MIT",
  "main": "lib/commonjs/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "bob build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wavyapp/react-native-star-prnt.git"
  },
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "android",
    "lib",
    "ios",
    "ReactNativeStarPrinter.podspec",
    "src",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "keywords": [
    "react-native",
    "ecosystem:react-native",
    "react-ios",
    "react-android",
    "print",
    "printer",
    "Star",
    "StarPRNT",
    "react-bridge",
    "ios",
    "android"
  ],
  "devDependencies": {
    "react-native": "^0.73.4",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react-native": "^0.73.4"
  },
  "packageManager": "yarn@4.2.2"
}
